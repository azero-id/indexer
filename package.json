{
  "name": "@azns/indexer",
  "author": "AZERO.ID <hello@azero.id> (https://azero.id)",
  "homepage": "https://azero.id",
  "private": true,
  "engines": {
    "node": "18"
  },
  "scripts": {
    "build": "rm -rf lib && tsc",
    "update": "npx npm-check-updates --filter /subsquid/ --upgrade && npm i -f",
    "codegen:model": "squid-typeorm-codegen",
    "codegen:metadata": "./typegen-all.sh",
    "codegen": "npm run codegen:model && npm run codegen:metadata",
    "migration:generate": "npm run build && squid-typeorm-migration generate",
    "migration:apply": "squid-typeorm-migration apply",
    "start:archive": "docker compose -f docker-compose.archive.yml up",
    "start:squid": "docker compose -f docker-compose.squid.yml up",
    "start:processor": "npm run build && npm run migration:apply && node -r dotenv/config lib/processor",
    "serve": "squid-graphql-server",
    "lint": "prettier . --check && eslint",
    "lint:fix": "prettier . --write && eslint --fix",
    "type-check": "tsc --pretty --noEmit"
  },
  "dependencies": {
    "@polkadot/util": "^12.6.1",
    "@subsquid/archive-registry": "^3.3.0",
    "@subsquid/graphql-server": "4.3.1",
    "@subsquid/ink-abi": "^3.0.1",
    "@subsquid/ss58": "^2.0.1",
    "@subsquid/substrate-processor": "^7.2.1",
    "@subsquid/typeorm-migration": "1.2.2",
    "@subsquid/typeorm-store": "^1.2.4",
    "axios": "^1.6.2",
    "axios-retry": "^3.9.1",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "pg": "^8.11.3",
    "pino": "^8.16.2",
    "pino-pretty": "^10.2.3",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@subsquid/ink-typegen": "^3.0.2",
    "@subsquid/substrate-metadata-explorer": "^3.1.1",
    "@subsquid/typeorm-codegen": "^1.3.2",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.1.0",
    "type-graphql": "^1.2.0-rc.1",
    "typescript": "^5.3.2"
  }
}
