{
  "name": "@azns/indexer",
  "author": "AZERO Domains <hello@azero.domains> (https://azero.domains)",
  "homepage": "https://azero.domains",
  "engines": {
    "node": "16"
  },
  "scripts": {
    "codegen:model": "squid-typeorm-codegen",
    "codegen:metadata": "./typegen-all.sh",
    "codegen": "npm run codegen:model && npm run codegen:metadata",
    "build": "rm -rf lib && tsc",
    "migration:generate": "npm run build && rm -rf ./db/migrations && squid-typeorm-migration generate",
    "migration:apply": "squid-typeorm-migration apply",
    "start:archive": "docker compose -f docker-compose.archive.yml up",
    "start:squid": "docker compose -f docker-compose.squid.yml up",
    "start:processor": "npm run build && npm run migration:apply && node -r dotenv/config lib/processor",
    "serve": "squid-graphql-server",
    "lint": "prettier . --check && eslint",
    "lint:fix": "prettier . --write && eslint --fix"
  },
  "devDependencies": {
    "@subsquid/archive-registry": "^2.1.11",
    "@subsquid/graphql-server": "3.3.2",
    "@subsquid/ink-abi": "^1.1.0",
    "@subsquid/ink-typegen": "^0.2.1",
    "@subsquid/ss58": "^0.1.4",
    "@subsquid/substrate-metadata-explorer": "^1.1.2",
    "@subsquid/substrate-processor": "2.4.1",
    "@subsquid/typeorm-codegen": "^0.3.3",
    "@subsquid/typeorm-migration": "0.1.6",
    "@subsquid/typeorm-store": "0.2.2",
    "@types/node": "^16.18.23",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "dotenv": "^16.0.3",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "pg": "^8.10.0",
    "prettier": "^2.8.7",
    "rimraf": "^4.4.1",
    "ts-node": "^10.9.1",
    "typeorm": "^0.3.14",
    "typescript": "^4.9.5"
  }
}
